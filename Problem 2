int main(){
    int input,i,n1=1,n2;
    std::cin>>input;
    std::cout<<n1<<" ";
    for(i=1;i<=input-1;i++){
        if(input%2!=0){
          n2=n1+2;
          std::cout << n2<<" ";
          n1=n2;
          n2=n1;
        }
        else{
            if(i<=input-2){
                n2=n1+2;
                std::cout << n2<<" ";
                n1=n2;
                n2=n1;
            }
            
        }
    }
    return 0;
}
